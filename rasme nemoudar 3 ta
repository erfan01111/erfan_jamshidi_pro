plt.fill_between(x, y, 0, where=(y < 0), color='skyblue', alpha=1)

plt.figure(figsize=(5, 4))
y3 = np.exp(-(x**2))
y2 = np.cos(1.2*x)
plt.plot(x, y2,'g--', label='cos(1.2*x)')
plt.plot(x, y3, 'r', label= 'exp(-x**3)')
plt.xlabel('x')
plt.ylabel('f(x)')
plt.legend()
plt.grid(True)