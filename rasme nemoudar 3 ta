import numpy as np
import matplotlib.pyplot as plt

x = np.linspace(-5 , 5, 500)
y1= (x**3)
plt.figure(figsize=(4, 3))
plt.plot(x, y1,'r--', label='x**3')
plt.title('y1 = x**3')
plt.xlabel('x')
plt.ylabel('f(x)')
plt.legend()
